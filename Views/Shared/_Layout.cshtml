@{
    var user = Context.User;
 
    var emailname = user.Identity.Name;
    var name = "Guest";


    // Print all claims for debugging
    var allClaims = user.Claims.ToList();
    foreach (var claim in allClaims)
    {
        if (claim.Type == "name")
        {
            name = claim.Value;
            break;
        }
    }
}

